{
  "name": "@bufferapp/session-manager",
  "version": "0.8.2",
  "description": "Buffer session manager",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js test/ src/",
    "test": "npm run lint && jest",
    "test-watch": "jest --watch",
    "build": "babel src -d lib",
    "prepublish": "babel src -d lib"
  },
  "author": "hharnisc@gmail.com",
  "dependencies": {
    "jsonwebtoken": "8.2.2",
    "@bufferapp/micro-rpc-client": "0.1.5",
    "object-path": "^0.11.8"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8"
          },
          "browsers": "> 3%"
        }
      ]
    ]
  },
  "files": [
    "lib",
    "src"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "es6": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        2,
        {
          "singleQuote": true,
          "trailingComma": "all",
          "semi": false,
          "bracketSpacing": true
        }
      ],
      "no-unused-vars": 2
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^26.6.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^26.6.3",
    "prettier": "^1.14.2"
  }
}
